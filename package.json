{
  "name": "vite-plugin-switch-hosts",
  "version": "0.0.7",
  "description": "switch hosts by service worker for each origin in browser without modify system hosts file",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "src",
    "dist",
    "pnpm-lock.yaml",
    "tsup.config.ts",
    "tsconfig.json"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "prepare": "husky install",
    "build": "tsup"
  },
  "keywords": [
    "vite",
    "vite-plugin",
    "switch-hosts",
    "SwitchHosts",
    "host",
    "hosts",
    "switch"
  ],
  "author": "lisonge",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lisonge/vite-plugin-switch-hosts.git"
  },
  "bugs": {
    "url": "https://github.com/lisonge/vite-plugin-switch-hosts/issues"
  },
  "homepage": "https://github.com/lisonge/vite-plugin-switch-hosts#readme",
  "dependencies": {
    "node-fetch": "2.6.7"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/node": "14.18.12",
    "@types/node-fetch": "^2.6.1",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "prettier": "^2.6.0",
    "ts-node": "^10.7.0",
    "tsup": "^5.12.1",
    "typescript": "^4.6.2",
    "vite": "^2.8.6"
  },
  "peerDependencies": {
    "vite": "^2.0.0"
  },
  "engines": {
    "pnpm": "^6.0.0"
  }
}
